{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "DetailedErrors": true,
  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Url": "https://*:6001" // Replace 6001 with the port you want to use
      }
    }
  }
}
